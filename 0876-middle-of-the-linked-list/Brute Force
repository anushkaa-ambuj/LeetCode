// Time Complexity: O(N^2)
// Space Complexity: O(1)

Node * middleNode2(Node * head) {
  if (head == NULL || head -> next == NULL) return head;
  int len = 0;
  Node * temp = head;
  while (temp != NULL) {
    len++;
    temp = temp -> next;
  }
  int mid = len / 2;
  temp = head;
  while (mid--) {
    temp = temp -> next;
  }
  return temp;
}
